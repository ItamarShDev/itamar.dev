{
    "name": "learn-starter",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "NODE_OPTIONS='--inspect' next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "storybook": "start-storybook -p 6006 --no-dll",
        "build-storybook": "build-storybook --no-dll",
        "prepare": "husky install"
    },
    "dependencies": {
        "chart.js": "^3.9.1",
        "gray-matter": "^4.0.3",
        "next": "^13.2.3",
        "react": "^18.2.0",
        "react-chartjs-2": "^4.3.1",
        "react-dom": "^18.2.0",
        "sharp": "^0.30.7"
    },
    "devDependencies": {
        "@types/node": "18.14.5",
        "@types/react": "18.0.28",
        "eslint": "^8.23.0",
        "eslint-config-next": "^13.2.3",
        "eslint-config-prettier": "^8.5.0",
        "husky": ">=8",
        "lint-staged": ">=13",
        "marked": "^4.1.0",
        "prettier": "2.7.1",
        "prism-react-renderer": "^1.3.5",
        "typescript": "^4.8.2"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,css,md}": "npm run lint --file",
        "*.js": "eslint --cache --fix",
        "*.{js,css,md}": "prettier --write"
    }
}
